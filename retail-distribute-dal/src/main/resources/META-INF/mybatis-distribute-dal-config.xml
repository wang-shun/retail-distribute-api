<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<!-- changes from the defaults -->
		<setting name="lazyLoadingEnabled" value="false"/>
		<!-- JdbcType enumeration. Most common are: NULL, VARCHAR and OTHER -->
		<setting name="jdbcTypeForNull" value="NULL"/>
		<setting name="defaultStatementTimeout" value="120"/>
	</settings>
	
	<mappers>

  <mapper resource="mapper/distribute/DistributeCltypebNewMapper.xml"/>
  <mapper resource="mapper/distribute/DistributeCltypeNewMapper.xml"/>
  <mapper resource="mapper/distribute/DistributeCltypepMapper.xml"/>
  <mapper resource="mapper/distribute/DistributeColothMapper.xml"/>
  <mapper resource="mapper/distribute/DistributeColothTBMapper.xml"/>
  <mapper resource="mapper/distribute/DistributeColothTMapper.xml"/>
  <mapper resource="mapper/distribute/DistributeCnfnMapper.xml"/>
  </mappers>
</configuration>
